/*
Найти наименьшее положительное целое число, квадрат которого заканчивается цифрами 269696.
*/

#include <iostream>

int main()
{   int r=0, ans=0, S=1, i=524;

    do{
        S=i*i;
        ans=(S%1000000==269696)?i:0;
        i+=1;
        r+=1;
    }while(ans==0);
    
    std::cout<<"ans="<< ans<<" r="<<r;
   

    return 0;
}
